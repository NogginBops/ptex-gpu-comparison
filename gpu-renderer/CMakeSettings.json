      "ctestCommandArgs": "",
      "cmakeToolchain": "${projectDir}/lib/ptex/vcpkg/scripts/buildsystems/vcpkg.cmake",
      "inheritEnvironments": [ "linux_x64" ],
      "wslPath": "${defaultWSLPath}",
      "variables": []
